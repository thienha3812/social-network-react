{
  "name": "test",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest",
    "test:CI": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll",
    "open:cover": "npm run test:cover && open-cli ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "reactide-server": "webpack-dev-server --config reactide.config.js --mode development --hot",
    "lint:fix": "eslint --fix --ext .js,.jsx ."
  },
  "author": "HaoNhien",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "4.1.0",
    "@hot-loader/react-dom": "16.10.2",
    "@livechat/ui-kit": "0.4.0-10",
    "@reduxjs/toolkit": "1.3.5",
    "@svgr/webpack": "5.4.0",
    "antd": "4.2.0",
    "axios": "0.21.1",
    "bootstrap": "4.4.1",
    "classnames": "2.2.6",
    "connected-react-router": "6.5.2",
    "google-map-react": "1.1.7",
    "html-entities": "1.3.1",
    "lodash": "4.17.15",
    "object-assign": "4.1.1",
    "react": "^16.8",
    "react-bootstrap": "1.0.1",
    "react-dom": "^16.8",
    "react-is": "^16.8",
    "react-lazy-load-image-component": "1.5.0-beta.0",
    "react-lazyload": "2.6.7",
    "react-localstorage": "1.0.0",
    "react-redux": "7.1.1",
    "react-responsive-carousel": "3.2.7",
    "react-router-dom": "5.1.2",
    "reactide-config": "1.0.2",
    "redux": "4.0.4",
    "redux-localstorage": "0.4.1",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-constant-elements": "7.6.3",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "7.6.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0-beta.0",
    "acorn": "7.1.0",
    "ajv": "6.10.2",
    "autoprefixer": "9.7.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.7",
    "chalk": "2.4.2",
    "concurrently": "5.0.0",
    "connect-history-api-fallback": "1.6.0",
    "coveralls": "3.0.7",
    "css-loader": "^2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "eslint-watch": "6.0.1",
    "file-loader": "^4.0.0",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.8.0",
    "mockdate": "2.0.5",
    "node-sass": "^4.12.0",
    "open-cli": "5.0.0",
    "postcss-loader": "3.0.0",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.12.15",
    "react-test-renderer": "16.11.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.1.1",
    "rimraf": "3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "^3.7.0",
    "webpack-hot-middleware": "2.25.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
}
